[
  {
    "label": "SF::Org::Picker",
    "command": "./sf-tui",
    "use_new_terminal": false,
    "allow_concurrent_runs": true,
    "reveal": "always"
  },
  {
    "label": "SF::Deploy",
    "command": "sf project deploy start --source-dir $ZED_FILE",
    "use_new_terminal": false,
    "allow_concurrent_runs": true,
    "reveal": "always"
  },
  {
    "label": "SF::Retrieve",
    "command": "sf project retrieve start --source-dir $ZED_FILE",
    "use_new_terminal": false,
    "allow_concurrent_runs": true,
    "reveal": "always"
  },
  {
    "label": "SF::Retrieve::Manifest",
    "command": "sf project retrieve start --manifest $ZED_FILE",
    "use_new_terminal": false,
    "allow_concurrent_runs": true,
    "reveal": "always"
  },
  {
    "label": "SF::Gen::LWC",
    "command": "./sf-tui gen --t LWC -o $ZED_WORKTREE_ROOT/force-app/main/default",
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
  {
    "label": "SF::Gen::ApexClass",
    "command": "./sf-tui gen --t ApexClass -o $ZED_WORKTREE_ROOT/force-app/main/default",
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
]
